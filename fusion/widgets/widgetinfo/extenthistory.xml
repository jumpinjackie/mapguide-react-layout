<WidgetInfo>
  <Type>ExtentHistory</Type>
  <LocalizedType>Extent History</LocalizedType>
  <Description>A widget to navigate forward or backward in the map view extent history</Description>
  <Label></Label>
  <ImageUrl></ImageUrl>
  <ImageClass></ImageClass>
  <Tooltip></Tooltip>
  <StandardUi>true</StandardUi>
  <ContainableBy>Any</ContainableBy>
  <Parameter>
    <Name>Direction</Name>
    <Description>The direction in which to traverse the extent history</Description>
    <Type>string</Type>
    <Label>Direction</Label>
    <AllowedValue>
      <Name>Next</Name>
      <Label>Next</Label>
    </AllowedValue>
    <AllowedValue>
      <Name>Previous</Name>
      <Label>Previous</Label>
    </AllowedValue>
    <DefaultValue>Previous</DefaultValue>
    <IsMandatory>true</IsMandatory>
  </Parameter>
</WidgetInfo>
