{
  "name": "mapguide-react-layout",
  "version": "0.14.7",
  "description": "A modern react-based map viewer for MapGuide",
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "yarn run clean:barrels && yarn run build:apidoc_npm && yarn run build:npm && yarn run build:bundle && yarn run build:storybook",
    "build:bundle": "yarn run clean:barrels && yarn run build:debug && yarn run build:prod && bestzip viewer.zip viewer/",
    "build:storybook": "rimraf ./storybook.zip && rimraf ./storybook-static && build-storybook -s ./src/stories/static -c .storybook && bestzip storybook.zip storybook-static/",
    "build:sprite": "glue stdassets/images/icons stdassets/sprites --html --png8 --sprite-namespace= --namespace=",
    "build:docs_sprite": "glue stdassets/images/icons docs_output --html --png8 --sprite-namespace= --namespace=",
    "build:barrels": "rimraf src/index.ts && barrelsby -d src --exclude src/entries --exclude src/stories",
    "build:dts": "rimraf index.d.ts && rimraf package && node tools/prepare-package.js && tsc --p tsconfig.dts.json",
    "build:npm": "rimraf package && node tools/prepare-package.js && tsc --p tsconfig.npm.json && copyfiles *.md lib/** src/** viewer/** stdassets/** package && rimraf lib",
    "build:site": "copyfiles docs_dev/content/** docs_output",
    "build:apidoc_npm_inner": "rimraf ./docs_output/latest/apidoc_npm && typedoc --options typedoc.json",
    "build:apidoc_npm": "yarn run build:apidoc_npm_inner && bestzip apidocs.zip docs_output/",
    "build:prod": "cross-env BUILD_MODE=production webpack --progress",
    "build:debug": "cross-env BUILD_MODE=development DEBUG_BUILD=1 webpack --progress",
    "build:dev": "cross-env BUILD_MODE=development webpack --progress",
    "build:json_schema": "typescript-json-schema tsconfig.npm.json ApplicationDefinition --out viewer/ApplicationDefinition.schema.json",
    "clean:barrels": "rimraf src/index.ts",
    "watch:site": "docsify serve docs_dev/content",
    "watch:dev": "yarn run clean:barrels && cross-env BUILD_MODE=development webpack -w --progress",
    "watch:test": "jest --watchAll",
    "stats:prod": "cross-env BUILD_MODE=production webpack -p --json > webpack_stats_prod.json",
    "stats:dev": "cross-env BUILD_MODE=development webpack --json > webpack_stats_dev.json",
    "clean": "rimraf viewer/dist/stdassets && rimraf viewer/dist/viewer.* && rimraf package/ && rimraf lib/",
    "lint": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'",
    "postinstall": "yarn run build",
    "storybook": "start-storybook -s ./src/stories/static -p 4000 -c .storybook",
    "test": "jest --runInBand --verbose",
    "test:e2e": "testcafe chrome e2e/test.*.ts -q -S -s e2e/screenshots",
    "test:appveyor": "jest --verbose --coverage --colors",
    "test:gha": "jest --runInBand --verbose --coverage --colors && cat ./coverage/lcov.info",
    "test:covrpt": "jest --coverage --coverageDirectory='coverage-report'",
    "ci:appveyor": "yarn run lint && yarn run test:appveyor",
    "ci:gha": "yarn run lint && yarn run test:gha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpinjackie/mapguide-react-layout.git"
  },
  "keywords": [
    "mapguide",
    "gis",
    "viewer"
  ],
  "author": "Jackie Ng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jumpinjackie/mapguide-react-layout/issues"
  },
  "homepage": "https://github.com/jumpinjackie/mapguide-react-layout#readme",
  "browserslist": "IE >= 10",
  "dependencies": {
    "@blueprintjs/core": "^3.54.0",
    "colorbrewer": "^1.5.3",
    "dompurify": "^2.4.2",
    "expr-eval": "2.0.2",
    "geojson-vt": "^3.2.1",
    "immutability-helper": "^3.1.1",
    "ismobilejs": "^1.1.1",
    "lodash.debounce": "^4.0.8",
    "ol": "6.14.1",
    "papaparse": "^5.3.2",
    "proj4": "2.8.0",
    "qs": "^6.10.5",
    "react": "^17.0.2",
    "react-colorful": "^5.6.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-rnd": "^10.3.5",
    "react-splitter-layout": "^4.0.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "stickybits": "3.7.11",
    "tslib": "^2.4.1",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addon-viewport": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@types/dom4": "2.0.2",
    "@types/dompurify": "^2.3.3",
    "@types/enzyme": "3.10.12",
    "@types/form-data": "2.5.0",
    "@types/geojson": "7946.0.8",
    "@types/jest": "27.4.1",
    "@types/lodash.debounce": "4.0.6",
    "@types/qs": "6.9.7",
    "@types/react": "17.0.39",
    "@types/react-addons-css-transition-group": "15.0.7",
    "@types/react-addons-update": "0.14.21",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "17.0.13",
    "@types/react-redux": "^7.1.23",
    "@types/topojson-specification": "1.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "barrelsby": "2.3.3",
    "bestzip": "2.2.0",
    "cache-loader": "^4.1.0",
    "copyfiles": "2.4.1",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "docsify-cli": "4.4.4",
    "enzyme": "3.11.0",
    "file-loader": "6.2.0",
    "jest": "27.5.1",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "jsonfile": "6.1.0",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "raf": "^3.4.1",
    "raw-loader": "4.0.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "testcafe": "^1.18.5",
    "testcafe-react-selectors": "^4.1.5",
    "thread-loader": "^3.0.4",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "tslint": "6.1.3",
    "typedoc": "0.22.14",
    "typescript": "4.6.3",
    "typescript-json-schema": "^0.53.0",
    "webpack": "5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "resolutions": {}
}
